msgid "Error reading server messsage (packet code: %1!d!, error: %2!ld!)."
msgstr "Napaka pri branju strežnikovega sporočila (koda paketa: %1!d!, napaka: %2!ld!)."

#: res/resource.rcx:331
msgid "Error removing installed XML profile \"%1!ls!\" (error: %2!ld!).%nPlease contact your network provider."
